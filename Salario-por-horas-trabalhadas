#include <stdio.h>
#include <stdlib.h>

int main(){
    float dias, s1, s2, s3, resultado ;

    printf("Uma empresa contratou um encanador por R$30,00 por dia\n");
    printf("\nInforme quantos dias o encanador trabalhou:\n\n");
    scanf("%f", &dias);


    s1 = dias * 30;
    s2 = s1 / 100;
    s3 = s2 * 8;
    resultado = s1 - s3;
    printf("O encanador devera receber:\n %.2f \n",resultado);


    return 0;
}
